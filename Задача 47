// Задайте двумерный массив размером m×n, заполненный случайными вещественными числами. m = 3, n = 4.

int[,] myArr = new int[3, 4];

Random ran = new Random();

for (int i = 0; i < 3; i++)
{
    for (int j = 0; j < 4; j++)
    {
       myArr[i, j] = ran.Next(1, 15);
       Console.Write("{0}\t", myArr[i, j]);
    }
    Console.WriteLine();
}
